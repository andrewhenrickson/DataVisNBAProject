{
  "buildCommand": "quasar build",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/css/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/js/$1"
    },
    {
      "src": "/fonts/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/fonts/$1"
    },
    {
      "src": "/viz/league/(.*\\.html)",
      "dest": "/viz/league/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
